{
  "name": "flowcon",
  "version": "2.0.16-beta.47",
  "description": "Flowcon: task flow",
  "repository": {
    "type": "git",
    "url": "https://github.com/oknosoft/flowcon"
  },
  "engines": {
    "node": "^8.8",
    "npm": "^5.5"
  },
  "keywords": [
    "metadata",
    "flowcon",
    "task flow",
    "task management",
    "time management"
  ],
  "author": "Ivan Belokamentsev <nmivan@oknosoft.ru> & Evgeniy Malyarov <info@oknosoft.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oknosoft/flowcon/issues"
  },
  "homepage": "https://flowcon.js.org/",
  "dependencies": {
    "metadata-abstract-ui": "^2.0.16-beta.47",
    "metadata-core": "^2.0.16-beta.47",
    "metadata-external": "^2.0.16-beta.47",
    "metadata-pouchdb": "^2.0.16-beta.47",
    "metadata-react": "^2.0.16-beta.47",
    "metadata-redux": "^2.0.16-beta.47",
    "metadata-superlogin": "^2.0.16-beta.47"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^2.3.0",
    "cors": "^2.8.4",
    "css-loader": "^0.28.9",
    "dotenv": "4.0.0",
    "eslint": "^4.16.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.6",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "md5-file": "^3.2.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.0.10",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6",
    "style-loader": "^0.19.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "^1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "superlogin": "node server",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint .",
    "meta:prebuild": "node scripts/meta.prebuild.js",
    "meta:link": "node scripts/meta.link.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
